{
  "job": {
    "content": [
      {
        "mysqlreader": {
          "type": "reader",
          "pluginName": "demoReader",
          "parameter": {
            "username": "duceap",
            "password": "123",
            "connection": [{
              "jdbcUrl": ["jdbc:mysql://10.254.10.31:3306/duceap_job_demo?useunicode=true&characterEncoding=utf8&zeroDateTimeBehavior=convertToNull&serverTimezone=Asia/Shanghai"],
              "table": ["job_execution_log"]
            }],
            "column": ["id","job_name","ip"],
            "customSql": "",
            "splitPk": "",
            "queryTimeOut": 1000,
            "requestAccumulatorInterval": 2
          }
        },
        "myprocessor1": {
          "type": "processor",
          "pluginName": "filterProcessor",
          "dependencies": ["mysqlreader"],
          "parameter": {
            "expression": "row.getField('age')>=1"
          }
        },
        "mywindow": {
          "type": "agg",
          "pluginName": "countWindowAgg",
          "dependencies": ["myprocessor1"],
          "parameter": {
            "windowSize": 5,
            "keyby": ["idcard"],
            "agg": [{
              "age": ["stats"],
              "sex": ["max"]
            }]
          }
        },
        "mywriter": {
          "type": "writer",
          "pluginName": "printWriter",
          "parameter": {
            "username": "duceap",
            "password": "123",
            "connection": [
              {
                "jdbcUrl": "jdbc:mysql://10.254.10.31:3306/duceap_job_demo?useunicode=true&characterEncoding=utf8&zeroDateTimeBehavior=convertToNull&serverTimezone=Asia/Shanghai",
                "table": ["job_execution_log2"]
              }
            ],
            "writeMode": "insert",
            "column": ["id","job_name","ip"],
            "batchSize": 1024
          }
        }
      }
    ],
    "setting":{
      "name": "myJob"
    }
  }
}

